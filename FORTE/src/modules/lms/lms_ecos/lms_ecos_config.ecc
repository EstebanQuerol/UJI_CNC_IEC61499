    # This option is not active
    # The parent CYGPKG_REDBOOT_ARM_LINUX_EXEC is not active

    # Flavor: bool
    # No user value, uncomment the following line to provide one.
    # user_value 0
    # value_source default
    # Default value: 0
};

# Physical base address of linux kernel
